{
  "author": "Spencer Kelly <spencermountain@gmail.com> (http://spencermounta.in)",
  "name": "fr-compromise",
  "description": "Computerlinguistik im browser",
  "version": "0.0.1",
  "main": "./builds/fr-compromise.mjs",
  "unpkg": "./builds/compromise.min.js",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./builds/fr-compromise.mjs",
      "require": "./builds/fr-compromise.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nlp-compromise/fr-compromise.git"
  },
  "scripts": {
    "test": "tape \"./tests/**/*.test.js\" | tap-dancer",
    "testb": "TESTENV=prod npm run test",
    "build": "rollup -c --silent",
    "pack": "node ./scripts/pack.js",
    "watch": "amble ./scratch.js",
    "score": "node ./learn/giga/test.js"
  },
  "files": [
    "builds/",
    "docs/"
  ],
  "dependencies": {
    "compromise": "14.2.0",
    "efrt": "2.6.0",
    "suffix-thumb": "4.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "3.1.9",
    "@rollup/plugin-node-resolve": "13.3.0",
    "amble": "1.3.0",
    "rollup": "2.75.5",
    "rollup-plugin-terser": "7.0.2",
    "tap-dancer": "0.3.4",
    "tape": "5.5.3",
    "xml-stream": "^0.4.5"
  },
  "license": "MIT"
}
